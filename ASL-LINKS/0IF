<script>

// The End Of Year Date To Countdown Date

document.addEventListener('DOMContentLoaded', () => {
    // Unix timestamp (in seconds) to count down to
    var toDayFromNow = (new Date("Dec 31, 2024 15:20:00").getTime() / 1000) + (3600 / 60 / 60 / 24) - 1;
    // Set Up FlipDown
    var flipdown = new FlipDown(toDayFromNow)
    // Start The Count Down
    .start()
    // Do Something When The Countdown Ends
    .ifEnded(() => {
        document.querySelector(".flipdown").innerHTML = `


      <div ><a href="https://mdiskshortner.link/mkhU9O"><img src="https://i.ibb.co/xLQR5gj/Picsart-24-11-22-08-07-06-437.jpg"width='100%'" loading="lazy" class="h-8 mt-5 m-auto opacity-100"><div class="mb-5 opacity-100"></div>
     
<button><b style='color:red;font-size:large;'>LIVE  NOW</button>




`;
    });
});

</script>
